    <!-- NetCoreApp2.1 ('lib\netcoreapp2.0' directory), Version 2.1 is copied to directory 2.0 to fix #224 -->
    <file src="..\..\ReportGenerator.DotnetCliTool\bin\Release\netcoreapp2.1\publish\**\*.*" target="lib\netcoreapp2.0" />
  </files>
</package>



